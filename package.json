{
  "name": "favorite_products",
  "version": "0.0.1",
  "description": "This is a NestJS project that provides an API for managing favorite products.",
  "author": "Walleks Miranda",
  "contributors": [
    "Walleks Miranda <walleksmiranda@gmail.com>"
  ],
  "private": false,
  "license": "UNLICENSED",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "up": "docker compose --env-file ./env/local.env up -d",
    "up:db": "docker compose --env-file ./env/local.env up db -d",
    "down": "docker compose --env-file ./env/local.env down",
    "local": "NODE_ENV=local nest start --watch",
    "dev": "NODE_ENV=development nest start --watch",
    "lint:migrate": "npx @eslint/migrate-config .eslintrc.js",
    "lint": " yarn eslint ./src/** --ignore-pattern \"src/main/config/typeorm/**\" --no-warn-ignored --fix",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:all": "npm test -- --detectOpenHandles -c jest-config-all.ts",
    "test:coverage": "npm run test -- --coverage",
    "test:unit": "npm run test -- --watchAll -c jest-unit-config.ts",
    "test:staged": "npm test -- -c jest-unit-config.ts",
    "seed:test": "NODE_ENV=test ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d database/seeds/typeorm-config-test.ts",
    "seed:local": "NODE_ENV=local ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d database/seeds/typeorm-config-test.ts",
    "seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d database/seeds/typeorm-config-test.ts",
    "typeorm:test": "NODE_ENV=test ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/main/config/typeorm/typeorm.config.ts",
    "typeorm:local": "NODE_ENV=local ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/main/config/typeorm/typeorm.config.ts",
    "typeorm:dev": "NODE_ENV=development ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/main/config/typeorm/typeorm.config.ts",
    "seed:test:create": "cd ./database/seeds/test && echo 'DIGITE O NOME DO SEED [SEM ESPAÇO]' && read name && NODE_ENV=test ../../../node_modules/.bin/typeorm migration:create $name && SEEDS=test npm run seed:import",
    "seed:import": "node ./database/scripts/seed-test-import.mjs",
    "seed:run:test": "npm run migration:run:test && npm run seed:test migration:run",
    "seed:run:local": "npm run migration:run:local && npm run seed:local migration:run",
    "seed:run:dev": "npm run migration:run:dev && NODE_ENV=development npm run schema:create && NODE_ENV=development npm run seed migration:run",
    "seed:drop:test": "NODE_ENV=test npm run seed:test schema:drop",
    "migration:run:test": "NODE_ENV=test npm run schema:create:test && npm run typeorm:test migration:run",
    "migration:create": "cd ./database/migrations && echo 'DIGITE O NOME DA MIGRATION [SEM ESPAÇO]' && read name && NODE_ENV=local ../../node_modules/.bin/typeorm migration:create $name && npm run migration:import",
    "migration:import": "node ./database/scripts/migration-import.mjs",
    "migration:run:local": "NODE_ENV=local npm run schema:create && npm run typeorm:local migration:run",
    "migration:revert:local": "npm run typeorm:local migration:revert",
    "migration:revert:test": "npm run typeorm:test migration:revert",
    "migration:run:dev": "NODE_ENV=development npm run schema:create && npm run typeorm:dev migration:run",
    "migration:revert:dev": "npm run typeorm:dev migration:revert",
    "schema:drop:local": "npm run typeorm:local schema:drop",
    "schema:drop:test": "npm run typeorm:test schema:drop",
    "schema:drop:dev": "npm run typeorm:dev schema:drop",
    "schema:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d database/create-schema query 'create schema if not exists favorite_products;'",
    "schema:create:test": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d database/create-schema query 'create schema if not exists favorite_products_test;'"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.4",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/typeorm": "10.0.0",
    "axios": "^1.9.0",
    "bull": "^4.16.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "nestjs-typeorm-paginate": "^4.0.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "5.0.0",
    "typeorm": "^0.3.17",
    "uuid": "^10.0.0",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/config-array": "^0.17.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.5",
    "@types/passport-jwt": "^3.0.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "git-commit-msg-linter": "^5.0.4",
    "globals": "^15.9.0",
    "jest": "^29.0.0",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.20",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm-codebase-sync": "^1.1.2",
    "typescript": "<=4.8",
    "typescript-eslint": "^8.0.0",
    "webpack": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
